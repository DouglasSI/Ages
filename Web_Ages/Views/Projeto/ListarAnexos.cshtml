@model List<Web_Ages.Models.ViewModel>

<div class="col-md-3" style="width: 2%"></div>
<div class="col-md-3" style="width: 96%">
    <div class="box-head">
        <h2 class="left">Itens Anexados</h2>
    </div>
    <div class="table">
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
            <tr>
                <th>
                    Título
                </th>
                <th>
                    Anotação
                </th>
                <th>
                    Criado em
                </th>
                <th>
                    Baixar
                </th>
                <th class="ac">Remover</th>
            </tr>
            @foreach (var item in (List<Web_Ages.Models.ViewModel>)@ViewBag.tb_anexo)
            {

                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.tb_anexo.titulo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.tb_anexo.anotacao)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.tb_anexo.data_cadastro)
                    </td>
                    <td>
                        Download/Visualizar
                    </td>
                </tr>
            }
        </table>
    </div>
</div>
