@model IEnumerable<Model.tb_projeto>
@{Layout = "~/Views/Shared/_Layout.cshtml";}
<div class="small-nav">
    <div>
        <a href="#">
            @Html.ActionLink("Página Inicial", "Index", "Home")
            <span>&gt;</span>
            Listas de Projetos
        </a>
    </div>
</div>
<div id="container">
    <div class="shell">
        @if (HttpContext.Current.User.Identity.IsAuthenticated)
        {
            <div id="main">
                <div id="content">
                    <!-- Box -->
                    @if (Model.Any())
                    {
                        <div class="msg msg-ok">
                            <p><strong>Projetos em Execução!</strong></p>
                        </div>
                        <div class="box">
                            <div class="box-head">
                                <h2 class="left">Projetos</h2>

                            </div>
                            <div class="table">
                                <table width="100%" border="0" cellspacing="0" cellpadding="0">
                                    <tr>

                                        <th>
                                            Titulo do Projeto
                                        </th>
                                        <th>
                                            Observação
                                        </th>
                                        <th>
                                            Valor Estimado
                                        </th>
                                        <th>
                                            Mantenedora
                                        </th>
                                        <th>
                                            Campi
                                        </th>
                                        <th>
                                            Situação
                                        </th>
                                        <th>
                                            Criado por
                                        </th>
                                        <th class="ac">Ações Disponiveis</th>
                                    </tr>

                                    @foreach (var item in Model)
                                    {
                                        <tr>

                                            <td><h3><a href="#">@Html.DisplayFor(modelItem => item.titulo)</a></h3></td>

                                            <td>
                                                @Html.DisplayFor(modelItem => item.anotacao)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.valor_estimado)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.tb_campi.tb_mantenedora.nome_fantasia)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.tb_campi.nome_fantasia)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.tb_status_projeto.descricao)
                                            </td>
                                            <td><a href="#">@Html.DisplayFor(modelItem => item.tb_usuario.nome)</a></td>
                                            <td>
                                                <a href="#" class="ico edit">@Html.ActionLink("Detalhes", "DetailsOrcamentosdoProjeto", new { id = item.id })</a>
                                            </td>
                                        </tr>
                                    }
                                </table>
                                <!-- Pagging -->
                                <!-- End Pagging -->
                            </div>
                        </div>
                    }
                    else
                    {
                        <div class="msg msg-ok">
                            <p><strong>Não Existe Projetos em Execução!</strong></p>
                        </div>
                    }
                    @if (((List<Model.tb_projeto>)ViewBag.Finalizado).Count > 0)
                    {
                        <div class="msg msg-ok">

                            <p><strong>Projetos Finalizados!</strong></p>

                        </div>
                        <div class="box">
                            <div class="box-head">
                                <h2 class="left">Projetos Finalizados</h2>

                            </div>
                            <div class="table">
                                <table width="100%" border="0" cellspacing="0" cellpadding="0">
                                    <tr>

                                        <th>
                                            Titulo do Projeto
                                        </th>
                                        <th>
                                            Observação
                                        </th>
                                        <th>
                                            Valor Estimado
                                        </th>
                                        <th>
                                            Mantenedora
                                        </th>
                                        <th>
                                            Campi
                                        </th>
                                        <th>
                                            Situação
                                        </th>
                                        <th>
                                            Criado por
                                        </th>
                                        <th class="ac">Ações Disponiveis</th>
                                    </tr>

                                    @foreach (var item in (List<Model.tb_projeto>)ViewBag.Finalizados)
                                    {
                                        <tr>

                                            <td><h3><a href="#">@Html.DisplayFor(modelItem => item.titulo)</a></h3></td>

                                            <td>
                                                @Html.DisplayFor(modelItem => item.anotacao)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.valor_estimado)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.tb_campi.tb_mantenedora.nome_fantasia)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.tb_campi.nome_fantasia)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.tb_status_projeto.descricao)
                                            </td>
                                            <td><a href="#">@Html.DisplayFor(modelItem => item.tb_usuario.nome)</a></td>
                                            <td>
                                                <a href="#" class="ico edit">@Html.ActionLink("??", "??", new { id = item.id })</a>
                                            </td>
                                        </tr>
                                    }
                                </table>

                            </div>
                        </div>
                    }

                    <!-- End Box -->
                </div>
            </div>
        }
    </div>
</div><br /><br /><br />.